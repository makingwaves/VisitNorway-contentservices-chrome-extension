{
    "name": "VisitNorwayCS-v1.3",
    "version": "1.3",
    "manifest_version": 2,
    "description": "Language selector",
    "permissions": [
        "storage",
        "tabs",
        "activeTab",
        "http://*/*",
        "https://*/*"
    ],
    "page_action": {
        "default_icon": "icons/favicon-48.png"
    },
    "background": {
        "scripts": ["background.js"]
    },
    "options_ui": {
        "page": "options.html"
    }
}